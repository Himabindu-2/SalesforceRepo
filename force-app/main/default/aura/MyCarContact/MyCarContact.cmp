<aura:component implements="flexipage:availableForAllPageTypes,forceCommunity:availableForAllPageTypes" access="global" >

<aura:attribute name="contactId" type="string"></aura:attribute>

    <aura:attribute name="MsgSuccess" type="boolean" default="false"></aura:attribute>

    
    <aura:if isTrue="{!v.MsgSuccess}">

        

            <aura:set attribute="else">

            <div class="slds-p-around_small">

                <lightning:recordEditForm objectApiName="CONTACT__c" ><!--</lightning:recordEditForm>onsuccess="{!c.handlesSuccess}">-->

                    
                        <lightning:inputField fieldName="Name"  variant="label-stacked" ></lightning:inputField>

                        <lightning:inputField fieldName="Email__c"  variant="label-stacked" ></lightning:inputField>

                        <lightning:inputField fieldName="Phone__c"  variant="label-stacked" ></lightning:inputField>

                         <lightning:inputField fieldName="Driving_License__c"  variant="label-stacked" ></lightning:inputField>
                    
                        <lightning:inputField fieldName="Driving_Experience__c"  variant="label-stacked" ></lightning:inputField>
                    
                       
                     <!--   <lightning:inputField fieldName="Booking_Status__c"  variant="label-stacked" ></lightning:inputField>
                                     
                         <lightning:inputField fieldName="Rented_Date__c"  variant="label-stacked" ></lightning:inputField>
                          -->
                    
                        

                       <lightning:button type="submit" name="update" label="Submit Contact" variant="brand" class="slds-m-top__small"></lightning:button>
                
                    </lightning:recordEditForm>

                </div>

            </aura:set>

        </aura:if>

    

</aura:component>